    <!-- "Furniture" obstacles -->
    <include>
      <uri>model://table</uri>
      <pose>2.0 1.5 0 0 0 0</pose>
    </include>

    <include>
      <uri>model://bookshelf</uri>
      <pose>-3.0 4.89 0 0 0 0</pose>
    </include>

    <include>
      <uri>model://cardboard_box</uri>
      <pose>-3.0 0.89 3.0 0 0 0</pose>
    </include>

    <include>
