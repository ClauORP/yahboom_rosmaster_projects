    <!-- Inner walls (make it "home-like") -->
    <model name="wall_inner_1">
      <static>true</static>
      <pose>-1.0 0.5 0.5 0 0 0</pose>
      <link name="link">
        <collision name="col">
          <geometry>
            <box>
              <size>0.2 6 1</size>
            </box>
          </geometry>

          <surface>
            <bounce>
              <!-- kill bounce -->
              <restitution_coefficient>0.0</restitution_coefficient>
