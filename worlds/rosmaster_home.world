      <pose>2.0 -1.0 0.5 0 0 0</pose>
      <link name="link">
        <collision name="col">
          <geometry>
            <box>
              <size>6 0.2 1</size>
            </box>
          </geometry>

          <surface>
            <bounce>
              <!-- kill bounce -->
              <restitution_coefficient>0.0</restitution_coefficient>
              <threshold>1e6</threshold>
            </bounce>
            <friction>
              <ode>
                <!-- reasonable friction  -->
                <mu>0.5</mu>
                <mu2>0.5</mu2>
              </ode>
            </friction>
            <contact>
              <ode>
                <!-- contact stiffness/damping -->
                <kp>200000</kp>
                <kd>200</kd>
                <!-- IMPORTANT: caps the separation correction speed (prevents "launching") -->
                <max_vel>0.05</max_vel>
                <!-- allow a tiny penetration before aggressive correction -->
                <min_depth>0.003</min_depth>
                <soft_cfm>0.00001</soft_cfm>
                <soft_erp>0.2</soft_erp>
              </ode>
            </contact>
          </surface>
        </collision>
        <visual name="vis">
